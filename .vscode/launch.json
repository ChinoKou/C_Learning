        {
            "name": "(Windows) 启动",
            "preLaunchTask": "C/C++: gcc.exe 生成活动文件",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "args": [
                "-c",
                "${workspaceFolder}\\build\\${fileBasenameNoExtension}.exe;",
                "Write-Output `n",
            ],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "console": "externalTerminal"
        }
    ]
}